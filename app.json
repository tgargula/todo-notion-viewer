{
  "expo": {
    "name": "Notodo",
    "slug": "notodo",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/notion.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/notion.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/notion.png"
      },
      "package": "dev.tgargula.notodo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
